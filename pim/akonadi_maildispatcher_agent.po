#: maildispatcheragent.cpp:95 maildispatcheragent.cpp:264
#, kde-format
msgid "Sending messages (1 item in queue)..."
msgid_plural "Sending messages (%1 items in queue)..."
msgstr[0] "傳送信件中（佇列中有 %1 個項目）"

#: maildispatcheragent.cpp:106
#, kde-format
msgid "Sending canceled."
msgstr "傳送已取消。"

#: maildispatcheragent.cpp:113
#, kde-format
msgid "Finished sending messages."
msgstr "已完成傳送信件。"

#: maildispatcheragent.cpp:118
#, kde-format
msgctxt "Notification title when email was sent"
msgid "E-Mail Successfully Sent"
msgstr "電子郵件已成功傳送"

#: maildispatcheragent.cpp:119
#, kde-format
msgctxt "Notification when the email was sent"
msgid "Your E-Mail has been sent successfully."
msgstr "您的電子郵件已成功傳送。"

#: maildispatcheragent.cpp:125
#, kde-format
msgid "No items in queue."
msgstr "佇列中沒有任何項目。"

#: maildispatcheragent.cpp:190
#, kde-format
msgid "Online, sending messages in queue."
msgstr "回到線上，傳送佇列中的信件中。"

#: maildispatcheragent.cpp:194
#, kde-format
msgid "Offline, message sending suspended."
msgstr "已離線，傳送信件動作已暫停。"

#: maildispatcheragent.cpp:218
#, kde-format
msgctxt "Message with given subject is being sent."
msgid "Sending: %1"
msgstr "傳送中：%1"

#: maildispatcheragent.cpp:289
#, kde-format
msgctxt "Notification title when email sending failed"
msgid "E-Mail Sending Failed"
msgstr "傳送信件失敗"

#: maildispatcheragent.cpp:322
#, kde-format
msgid "Ready to dispatch messages."
msgstr "已準備好發送信件。"

#. i18n: ectx: label, entry (Outbox), group (General)
#: maildispatcheragent.kcfg:10
#, kde-format
msgid "Outbox collection id"
msgstr "寄件匣收藏代碼"

#. i18n: ectx: label, entry (SentMail), group (General)
#: maildispatcheragent.kcfg:14
#, kde-format
msgid "Sent Mail collection id"
msgstr "寄件備份收藏代碼"

#: outboxqueue.cpp:278
#, kde-format
msgid "Could not access the outbox folder (%1)."
msgstr "無法存取寄件匣（%1）。"

#: sendjob.cpp:62 sendjob.cpp:63
#, kde-format
msgid "Message sending aborted."
msgstr "傳送信件已中止。"

#: sendjob.cpp:71
#, kde-format
msgid "Could not initiate message transport. Possibly invalid transport."
msgstr "無法初始化信件傳送。可能使用了不正確的傳輸。"

#: sendjob.cpp:77
#, kde-format
msgid "Could not send message. Invalid transport."
msgstr "無法傳送信件。可能使用了不正確的傳輸。"

#: sendjob.cpp:109
#, kde-format
msgid "Failed to get D-Bus interface of resource %1."
msgstr "無法取得資源 %1 的 D-Bus 介面。"

#: sendjob.cpp:124
#, kde-format
msgid "Invalid D-Bus reply from resource %1."
msgstr "從資源 %1 得到不合法的 D-Bus 回覆。"

#: sendjob.cpp:232
#, kde-format
msgid "Default sent-mail folder unavailable. Keeping message in outbox."
msgstr "無法取得預設的寄件備份資料夾。信件將保留在寄件匣中。"

#: sendjob.cpp:244
#, kde-format
msgid "Message transport aborted."
msgstr "信件傳輸已中止。"

#: sendjob.cpp:245
#, kde-format
msgid "Failed to transport message."
msgstr "傳送信件失敗。"

#: sendjob.cpp:287
#, kde-format
msgid "Failed to get D-Bus interface of mailfilteragent."
msgstr "無法取得郵件過濾代理的 D-Bus 介面。"

#: sendjob.cpp:294
#, kde-format
msgid "Invalid D-Bus reply from mailfilteragent"
msgstr "從郵件過濾代理得到不合法的 D-Bus 回覆。"

#: sendjob.cpp:337
#, kde-format
msgid "Sending succeeded, but failed to remove the message from the outbox."
msgstr "傳送成功，但是在將信件從寄件匣中移除時發生錯誤。"

#: sendjob.cpp:341
#, kde-format
msgid ""
"Sending succeeded, but failed to move the message to the sent-mail folder."
msgstr "傳送成功，但是在將信件移動到寄件備份時發生錯誤。"

#: sendjob.cpp:377
#, kde-format
msgid "Failed to store result in item."
msgstr "儲存結果時失敗。"

#: storeresultjob.cpp:76
#, kde-format
msgid "Failed to fetch item."
msgstr "抓取項目時失敗。"

